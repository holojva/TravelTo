<?xml version="1.0" encoding="utf-8"?>

<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#fde4c2">


    <TextView
        android:id="@+id/placeName"
        android:layout_width="match_parent"
        android:layout_height="45dp"
        android:layout_marginStart="31dp"
        android:layout_marginEnd="34dp"
        android:layout_marginBottom="13dp"
        android:background="#E8C25B"
        android:fontFamily="@font/unbounded_bold"
        android:gravity="center"
        android:padding="0px"
        android:text="Гора Арарат"
        android:textAlignment="gravity"
        android:textSize="20sp"
        app:layout_constraintBottom_toTopOf="@+id/placeAddress14"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/web_view_img" />

    <TextView
        android:id="@+id/placeAddress"
        android:layout_width="match_parent"
        android:layout_height="58dp"
        android:layout_marginStart="32dp"
        android:layout_marginTop="124dp"
        android:layout_marginEnd="34dp"
        android:layout_marginBottom="14dp"
        android:fontFamily="@font/unbounded"
        android:gravity="center"
        android:padding="0px"
        android:text="Беларусь, Минск, проспект Независимости, 171"
        android:textAlignment="gravity"
        app:layout_constraintBottom_toTopOf="@+id/placeAddress13"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/placeName" />

    <TextView
        android:id="@+id/placeAddress5"
        android:layout_width="match_parent"
        android:layout_height="58dp"
        android:layout_marginStart="32dp"
        android:layout_marginTop="56dp"
        android:layout_marginEnd="34dp"
        android:fontFamily="@font/unbounded"
        android:layout_marginBottom="10dp"
        android:background="#E8C25B"
        android:gravity="center"
        android:padding="0px"
        android:text="Беларусь, Минск, проспект Независимости, 171"
        android:textAlignment="gravity"
        app:layout_constraintBottom_toTopOf="@+id/placeAddress"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/placeName" />

    <TextView
        android:id="@+id/placeAddress13"
        android:layout_width="match_parent"
        android:layout_height="58dp"
        android:layout_marginStart="32dp"
        android:layout_marginTop="196dp"
        android:layout_marginEnd="34dp"
        android:fontFamily="@font/unbounded"
        android:layout_marginBottom="134dp"
        android:background="#FDE4C2"
        android:gravity="center"
        android:padding="0px"
        android:text="Беларусь, Минск, проспект Независимости, 171"
        android:textAlignment="gravity"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/placeName" />

    <TextView
        android:id="@+id/placeAddress14"
        android:layout_width="match_parent"
        android:layout_height="32dp"
        android:layout_marginStart="32dp"
        android:layout_marginEnd="36dp"
        android:layout_marginBottom="12dp"
        android:background="#E8C25B"
        android:gravity="center"
        android:padding="0px"
        android:text="Достопримечательность"
        android:fontFamily="@font/unbounded"
        android:textAlignment="gravity"
        app:layout_constraintBottom_toTopOf="@+id/placeAddress5"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/placeName" />

    <WebView
        android:id="@+id/web_view_img"
        android:layout_width="match_parent"
        android:layout_height="244dp"
        android:layout_marginStart="34dp"
        android:layout_marginTop="30dp"
        android:layout_marginEnd="34dp"
        android:layout_marginBottom="24dp"
        app:layout_constraintBottom_toTopOf="@+id/placeName"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />
</androidx.constraintlayout.widget.ConstraintLayout>
